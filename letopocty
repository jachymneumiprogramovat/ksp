var fs = require("fs");
var inputText = fs.readFileSync("C:/Extrení disk - Lokálně/zalohy/Jachym/kód/ksp2021/01.in").toString()
//tady rozděluju input abych s tím mohl normálně pracovat
inputLines = inputText.split("\n")
inputLines.shift()
inputLines.pop()


//finální výstup musí to být jako string, protože array by mi nešel napasovat rovnou do dalšího souboru a tohle dělá i hezky řádky tak jak je potřebují
var days = ""

function letopocty(){

for(let i of inputLines){
let day= 0;
let end = i.split(" ")[1]
let start =i.split(" ")[0]
end++
//tohle je to celé srdce. přidává to po jednom a čekuje podmínky, které jsou v zadání
for(;start!=end;start++){
            if(start%3==0&&start%48!=0){
                day+=1
            }
                day+=42
        }
        days+=day + "\n"
    }
    fs.writeFileSync("ksp2021kps-letopocty-vystup.txt", days,)
}
letopocty()
